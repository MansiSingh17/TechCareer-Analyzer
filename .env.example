# Location: /techcareer-analyzer/.env.example

# API Keys
ADZUNA_APP_ID=your_adzuna_app_id
ADZUNA_API_KEY=your_adzuna_api_key
LINKEDIN_EMAIL=your_email@example.com
LINKEDIN_PASSWORD=your_password
INDEED_PUBLISHER_ID=your_indeed_publisher_id

# Database
DATABASE_URL=postgresql://user:password@localhost:5432/techcareer
REDIS_URL=redis://localhost:6379

# Model Paths
BERT_MODEL_PATH=./data/models/bert_skill_extractor
SALARY_MODEL_PATH=./data/models/salary_predictor
FORECAST_MODEL_PATH=./data/models/skill_forecaster

# Server Config
API_HOST=0.0.0.0
API_PORT=8000
DEBUG_MODE=True

# ML Config
BATCH_SIZE=32
MAX_SEQ_LENGTH=512
LEARNING_RATE=2e-5
NUM_EPOCHS=10